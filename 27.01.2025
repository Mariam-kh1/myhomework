#include <iostream>
int main() {
    double a = 0;
    double b = 0;
    char c = '-';
    std :: cin >> a;
    std :: cin >> c;
    std :: cin >> b;
    switch (c) {
        case '+' :
            std :: cout << a + b << std ::  endl;
            break;
        case '-' :
            std :: cout << a - b << std :: endl;
            break;
        case '*':
            std :: cout << a * b << std ::  endl;
            break;
        case '/':
            if (0 != b) {
                std :: cout << a / b << std ::  endl;
            }else {
                std :: cout << "Error" << std ::  endl;
            }
            break;
        default:
            std :: cout << "Wrong sign" << std :: endl;
    }
    return 0;
}
